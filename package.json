    "name": "John Cole",
    "email": "johnwebbcole@gmail.com",
    "url": "http://github.com/johnwebbcole@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/johnwebbcole/jscad-utils.git"
  },
  "license": "ISC",
  "dependencies": {
    "@jscad/csg": "^0.7.0",
    "@jscad/scad-api": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "ava": "^2.2.0",
    "babelify": "^10.0.0",
    "del": "^5.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-ava": "^7.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-inject": "^5.0.4",
    "gulp-jscad-files": "^3.0.1",
    "gulp-jsdoc-to-markdown": "^1.2.1",
