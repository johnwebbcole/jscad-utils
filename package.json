    "gulp-terser": "^1.2.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^5.0.1",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/helpers/**/*.js"
