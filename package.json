{
  "name": "@jwc/jscad-utils",
  "version": "4.8.1",
  "description": "Utilities for use in a jscad script.",
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "src",
    "dist",
    "jscad.json"
  ],
  "scripts": {
    "test": "ava -v",
    "test:cov": "nyc --reporter=html --reporter=lcov --reporter=text ava",
    "build": "gulp all",
    "start": "gulp",
    "docs": "vuepress-jsdoc --source ./srcx --dist ./docs && npx vuepress build ./docs",
    "serve": "vuepress-jsdoc --source ./srcx --dist ./docs && vuepress dev ./docs"
  },
  "keywords": [
    "jscad"
  ],
  "homepage": "https://johnwebbcole.gitlab.io/jscad-utils",
  "bugs": "https://gitlab.com/johnwebbcole/jscad-utils/issues",
  "author": {
    "name": "John Cole",
    "email": "johnwebbcole@gmail.com",
    "url": "https://jwc.dev"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:johnwebbcole/jscad-utils.git"
  },
  "license": "ISC",
  "dependencies": {
    "@jscad/csg": "^0.7.0",
    "@jscad/scad-api": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "@jwc/jscad-test-utils": "^1.0.6",
    "@vuepress/plugin-active-header-links": "^1.5.2",
    "@vuepress/plugin-google-analytics": "^1.5.2",
    "ava": "^3.9.0",
    "del": "^5.0.0",
    "eslint": "^7.3.0",
    "eslint-plugin-ava": "^10.3.1",
    "fsevents": "^2.1.3",
    "gulp": "^4.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-inject": "^5.0.5",
    "gulp-jscad-files": "^4.1.0",
    "gulp-jsdoc-to-markdown": "^1.2.1",
    "gulp-load-plugins": "^2.0.3",
    "gulp-plumber": "^1.2.1",
    "gulp-terser": "^1.2.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^5.0.1",
    "nyc": "^15.1.0",
    "rollup": "^2.17.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "vuepress-jsdoc": "^3.0.0"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/helpers/**/*.js"
    ],
    "require": [
      "esm",
      "@babel/register",
      "babel-polyfill",
      "./test/helpers/compat-env.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "optionalDependencies": {},
  "peerDependencies": {
    "gl": "^4.3.3",
    "vuepress": "^1.0.3"
  }
}
